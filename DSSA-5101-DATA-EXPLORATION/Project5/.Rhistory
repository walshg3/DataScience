sort_printer_data_color <- function(df) {
dfsort <- aggregate(df$Cost, by=list(Category=df$MAJR_CODE_LIST), FUN=sum)
dfsort <- dfsort %>% arrange(desc(x))
dfsort <- dfsort %>% head(10)
return(dfsort)
}
